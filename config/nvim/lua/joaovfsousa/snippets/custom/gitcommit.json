{
  "conventional commit": {
    "prefix": "cc",
    "body": [
      "${1:type}(${2:scope}): ${3:title}",
      "",
      "Refs: ${4:MID}-$0"
    ]
  },
  "fix conventional commit": {
    "prefix": "fix",
    "body": [
      "fix(${1:scope}): ${2:title}",
      "",
      "Refs: ${3:MID}-$0"
    ]
  },
  "feat conventional commit": {
    "prefix": "feat",
