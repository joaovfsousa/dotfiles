(( $+commands[yarn] )) && export PATH="$PATH:`yarn global bin`"

if [[ -f "$HOME/.cargo/env" ]]; then
  . "$HOME/.cargo/env"
fi

# pnpm
export PNPM_HOME="/Users/joaovfsousa/Library/pnpm"
case ":$PATH:" in
  *":$PNPM_HOME:"*) ;;
  *) export PATH="$PNPM_HOME:$PATH" ;;
esac
# pnpm end


. /opt/homebrew/opt/asdf/libexec/asdf.sh
